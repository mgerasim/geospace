@model GeospaceEntity.Models.Settings
@{
    ViewBag.Title = "Настройки";
}

<h2>Настройки</h2>

<form class="form-horizontal">
    <div class="form-group">
        <label for="path_exe" class="col-sm-2 control-label">Путь запуска:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="path_exe" name="GeospaceTrackExe" value="@Model.path_exe" disabled>
        </div>
    </div>

    


    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            @Html.ActionLink("Редактировать", "Edit", new { id = Model.ID })
        </div>
    </div>
</form>
